{
    "name": "putyourlightson/craft-campaign",
    "description": "Send and manage email campaigns, contacts and mailing lists.",
    "version": "1.26.0",
    "type": "craft-plugin",
    "homepage": "https://putyourlightson.com/plugins/campaign",
    "license": "proprietary",
    "keywords": [
